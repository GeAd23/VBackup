    </style_image_button>
	<style_image_button layout_type_x="1" glow_obj="plane_obj1" color="1, 1, 1, 1" layout_type_y="0" label_obj="text_obj1" bg_obj="plane_obj2" image_obj="plane_obj3" image_margin="10" id="menu_style_button_action">
