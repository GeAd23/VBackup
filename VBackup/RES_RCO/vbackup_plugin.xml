      <textobj line_space="4" font_size="28" bold="1" align_y="1" align_x="1" ellipsis="1" id="text_obj1" />
      <planeobj scale_mode_y="0" scale_mode_x="0" texture0="tex_icon_unk" size="299, 80" id="plane_obj3" pos="0, 0" />
